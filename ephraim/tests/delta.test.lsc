test! 'rawSet', ->
  @entity() class E:
    @field() f

  e = create(E)
  expect(e~isDelta!).toBe(true)
  expect(e~hasChanges!).toBe(false)

  e~rawSet('f', 1)
  expect(e.f).toBe(1)
  expect(e~isDelta!).toBe(true)
  expect(e~hasChanges!).toBe(false)
  console.log(e)

  expect(e.g).toBe(undefined)
  e~rawSet('g', 2)
  console.log('after rawSet', e)
  expect(e.g).toBe(2)
  expect(e~isDelta!).toBe(true)
  expect(e~hasChanges!).toBe(false)

