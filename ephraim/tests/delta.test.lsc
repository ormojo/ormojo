test! 'subentity', ->
  @entity() class E:
    @field() e

  @entity() class F:
    @field(E) subE

  f = create(F)
  f.subE = create(E)
  f.subE.e = 1

  expect(f.subE.e).toBe(1)
  expect(f~toPlainObject!).toEqual({ subE: { e: 1 }})

test! 'subentity init', ->
  @entity() class E:
    @field() e = 1

  @entity() class F:
    @field(E) subE = {}

  f = create(F)

  expect(f.subE.e).toBe(1)
  expect(f~toPlainObject!).toEqual({ subE: { e: 1 }})
