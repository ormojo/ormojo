{
  "name": "@ormojo/elasticsearch",
  "version": "0.2.18",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "description": "Elasticsearch 7.x driver for ormojo.",
  "type": "commonjs",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib coverage .nyc_output",
    "build": "rollup -c",
    "build:coverage": "cross-env COVERAGE=true rollup -c",
    "test": "npm run clean && npm run build && jest",
    "test:only": "jest",
    "test:debug": "node --inspect --debug-brk $(npm bin)/jest -i --env jest-environment-node-debug",
    "coverage": "npm run clean && npm run build:coverage && jest --coverage",
    "lint": "eslint --ext .js,.lsc src",
    "preversion": "npm run lint && npm run build",
    "self-link": "mkdir -p node_modules/@ormojo && ln -s ../.. node_modules/@ormojo/ephraim-memory"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "@oigroup/babel-preset-lightscript": "^3.0.0",
    "@oigroup/lightscript-eslint": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "cross-env": "^5.0.5",
    "eslint": "~4.8.0",
    "jest": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "lsc"
    ],
    "transform": {
      "\\.lsc$": "<rootDir>/lightscript.jest.js"
    },
    "testRegex": "(\\.|/)test\\.lsc$"
  },
  "dependencies": {
    "@ormojo/ephraim": "^0.2.10",
    "@ormojo/ormojo": "^0.2.14",
    "deep-diff": "^1.0.2",
    "lodash": "^4.17.15"
  }
}
