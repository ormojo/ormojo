{
  "name": "@ormojo/elasticsearch",
  "version": "0.4.7",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "description": "Elasticsearch 7.x driver for ormojo.",
  "type": "commonjs",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib coverage .nyc_output",
    "build": "rollup -c",
    "build:coverage": "cross-env COVERAGE=true rollup -c",
    "test": "npm run clean && npm run build && jest",
    "test:only": "jest",
    "test:debug": "node --inspect --debug-brk $(npm bin)/jest -i --env jest-environment-node-debug",
    "coverage": "npm run clean && npm run build:coverage && jest --coverage",
    "lint": "eslint --ext .js,.lsc src",
    "preversion": "npm run lint && npm run build",
    "self-link": "mkdir -p node_modules/@ormojo && ln -s ../.. node_modules/@ormojo/ephraim-memory"
  },
  "dependencies": {
    "@ormojo/ephraim": "^1.0.3",
    "@ormojo/ormojo": "^1.1.2",
    "deep-diff": "^1.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "jest": "^27.0.4",
    "npm-run-all": "^4.1.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "lsc"
    ],
    "transform": {
      "\\.lsc$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    },
    "testRegex": "(\\.|/)test\\.lsc$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  }
}
