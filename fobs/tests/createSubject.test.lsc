import { createSubject, test } from '..'

describe! 'createSubject', (): void ->
  it! 'should work', ->
    subj = createSubject()

    test(subj, console.log.bind(console), [
      (x) -> x == 1
      (x) -> x == 2
      (x) -> x == 3
      'complete'
      'failure'
    ])

    subj.next(1); subj.next(2); subj.next(3); subj.complete()
