{
  "name": "ormojo",
  "version": "0.0.1",
  "description": "A multi-backend SQL/NoSQL ORM for node.js and the browser.",
  "main": "./index.js",
  "scripts": {
    "clean": "rm -rf js",
    "build": "mkdir -p js && coffee -o js src/*.coffee",
    "build_doc": "codo src/",
    "prepublish": "npm run clean && npm run build",
		"test": "node_modules/.bin/mocha --recursive --compilers coffee:coffee-script/register",
    "coverage": "node_modules/.bin/mocha --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul",
    "coverageReport": "node_modules/.bin/istanbul report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wcjohnson/ormojo.git"
  },
  "keywords": [
    "orm",
    "sql",
    "nosql",
    "denormalize"
  ],
  "author": "William C. Johnson <wcjohnson@oigroup.net> (https://github.com/wcjohnson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wcjohnson/ormojo/issues"
  },
  "homepage": "https://github.com/wcjohnson/ormojo",
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "coffee-coverage": "^1.0.1",
    "cuid": "^1.3.8",
    "istanbul": "^0.4.5",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "merge-options": "0.0.64"
  },
  "peerDependencies": {}
}
