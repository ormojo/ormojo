{
  "name": "ormojo-monorepo",
  "private": true,
  "version": "0.0.1",
  "description": "Lerna mono-repo for ormojo and related packages",
  "scripts": {
    "lerna": "lerna",
    "bootstrap": "lerna clean --yes && lerna bootstrap",
    "publish:some": "node scripts/releasePackages.js",
    "publish": "node scripts/releasePackages.js all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ormojo/ormojo.git"
  },
  "author": "William C. Johnson <wcjohnson@oigroup.net> (https://github.com/wcjohnson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ormojo/ormojo/issues"
  },
  "homepage": "https://github.com/ormojo/ormojo",
  "dependencies": {
    "lerna": "^3.4.1"
  },
  "workspaces": {
    "packages": [
      "ormojo",
      "elasticsearch",
      "mysql",
      "ephraim",
      "fobs",
      "tapestry",
      "react-observe"
    ]
  }
}
