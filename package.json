{
  "name": "ormojo-monorepo",
  "private": true,
  "version": "0.0.1",
  "description": "Lerna mono-repo for ormojo and related packages",
  "scripts": {
    "clean": "lerna run clean",
    "build": "lerna run build",
    "test": "lerna run test:only",
    "publish:execute": "lerna publish",
    "publish:full": "run-s build publish:execute"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ormojo/ormojo.git"
  },
  "author": "William C. Johnson <wcjohnson@oigroup.net> (https://github.com/wcjohnson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ormojo/ormojo/issues"
  },
  "homepage": "https://github.com/ormojo/ormojo",
  "dependencies": {
    "lerna": "^3.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@lightscript/babel-preset": "^4.0.0-alpha.31",
    "@lightscript/eslint-plugin": "^4.0.0-alpha.31",
    "babel-jest": "^26.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "find-parent-dir": "^0.3.0"
  },
  "workspaces": {
    "packages": [
      "ormojo",
      "elasticsearch",
      "mysql",
      "ephraim",
      "fobs",
      "tapestry",
      "react-observe",
      "data"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "lsc"
    ],
    "transform": {
      "\\.lsc$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(\\.|/)test\\.lsc$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  }
}
