import { entity, field, types, Entity } from '@ormojo/ephraim'

@entity({name: 'Timestamped'}) class Timestamped extends Entity:
  @field(types.date) createdAt
  @field(types.date) updatedAt

@entity({name: 'Tombstoned'}) class Tombstoned extends Entity:
  @field(types.date) deletedAt

export { Timestamped, Tombstoned }
