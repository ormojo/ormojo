{
  "name": "ormojo-reactive",
  "version": "0.2.0",
  "description": "Tools for reactive programming with ormojo.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib build es",
    "build": "npm run build:coffee && npm run build:babel:commonjs",
    "build:babel:commonjs": "cross-env BABEL_ENV=commonjs babel build --out-dir lib",
    "build:babel:istanbul": "mkdirp coverage/build && cross-env BABEL_ENV=istanbul babel build --out-dir coverage/build",
    "build:coffee": "mkdirp build && coffee --compile --output build/ src/",
    "prepublish": "npm run clean && npm run build",
    "test": "npm run test:mocha",
    "test:debug": "npm run test:mocha:debug",
    "test:mocha": "mocha --recursive lib/_tests_/*.js",
    "test:mocha:debug": "node --inspect --debug-brk node_modules/.bin/_mocha --recursive lib/_tests_/*.js",
    "coverage:build": "npm run build:babel:istanbul",
    "coverage:test": "node_modules/.bin/_mocha --recursive -- coverage/build/test/*.test.js",
    "coverage:report": "nyc --reporter=lcov npm run coverage:test",
    "coverage": "npm run build:coffee && npm run coverage:build && npm run coverage:report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ormojo/ormojo.git"
  },
  "keywords": [
    "orm",
    "ormojo",
    "rx",
    "observable"
  ],
  "author": "William C. Johnson <wcjohnson@oigroup.net> (https://github.com/wcjohnson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ormojo/ormojo/issues"
  },
  "homepage": "https://github.com/ormojo/ormojo",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "chai": "^3.5.0",
    "codo": "^2.1.2",
    "coffee-script": "^1.12.4",
    "cross-env": "^3.1.3",
    "cuid": "^1.3.8",
    "mocha": "^2.4.5",
    "rimraf": "^2.6.1",
    "mkdirp": "^0.5.1",
    "rxjs": "^5.0.3"
  },
  "dependencies": {
    "ormojo": "^0.2.0",
    "observable-utils": "^0.1.2"
  }
}
