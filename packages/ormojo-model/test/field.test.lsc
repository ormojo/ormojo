'use @oigroup/lightscript with bangCall'

import { Model, field } from '..'

class MyModel extends Model:
  @field({type: 'string'})
  x = 1

  par() -> 1

class NyModel extends MyModel:
  @field({type: 'integer'})
  y = 'hello'

  chi() -> 2

// recompile this jest
test! 'model prototype', ->
  mu = new MyModel
  nu = new NyModel
  debugger
  expect(mu.fields.x.type).toBe('string')
  expect(mu.fields.y).toBeFalsy()
  expect(nu.fields.y.type).toBe('integer')
