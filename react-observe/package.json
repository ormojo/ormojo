{
  "name": "@ormojo/react-observe",
  "version": "0.1.1",
  "license": "MIT",
  "description": "React components for monitoring observables.",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "jsnext:main": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib coverage .nyc_output",
    "clean:modules": "rimraf node_modules",
    "superclean": "run-s clean clean:modules",
    "build": "run-s build:rollup",
    "build:coverage": "run-s build:rollup:coverage",
    "build:rollup": "rollup -c",
    "build:plain": "babel --source-maps -x \".js\",\".lsc\" src --out-dir lib",
    "build:rollup:coverage": "cross-env COVERAGE=true rollup -c",
    "test": "run-s clean build test:only",
    "test:only": "jest",
    "test:only:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk $(npm bin)/jest -i",
    "coverage": "run-s clean build:coverage test:only:coverage",
    "lint": "eslint --ext .js,.lsc src",
    "lint:fix": "eslint --fix --ext .js,.lsc src"
  },
  "dependencies": {
    "@ormojo/fobs": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@lightscript/babel-preset": "4.0.0-alpha.18",
    "@lightscript/eslint-plugin": "^4.0.0-alpha.18",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-istanbul": "^5.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "lsc"
    ],
    "transform": {
      "\\.lsc$": "<rootDir>/scripts/lightscript.jest.js"
    },
    "testRegex": "(\\.|/)test\\.lsc$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
